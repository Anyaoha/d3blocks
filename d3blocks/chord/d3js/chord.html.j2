<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>{{ TITLE }}</title>
</head>
<body>


<script>
	{% include "chord.js" %}
    
    const names = {{ NAMES }};
    const colors = {{ COLORS }};
    const data = {{ json_data }};

    document.addEventListener('DOMContentLoaded', function () {
        window.Chord({
            names,
            colors,
            data
        });
    });

</script>
<div id="chart"></div>
</body>
</html>